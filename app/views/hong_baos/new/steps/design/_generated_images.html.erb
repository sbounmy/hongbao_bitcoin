<% Rails.logger.debug "🔍 Rendering generated_images with URLs: #{image_urls.inspect}" %>
<%= turbo_stream_from("ai_generations") %>
<div id="ai_generations" class="grid grid-cols-2 gap-4">
  <h4>Generated Designs</h4>
  <% if image_urls.present? %>
    <% image_urls.each do |url| %>
      <div class="relative aspect-square bg-white/10 rounded-lg overflow-hidden">
        <img src="<%= url %>"
             class="w-full h-full object-cover"
             alt="Generated design">
      </div>
    <% end %>
  <% else %>
    <p>No images generated yet</p>
  <% end %>
</div>
